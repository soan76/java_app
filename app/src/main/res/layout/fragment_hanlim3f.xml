<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ui.hanlim.Hanlim3fFragment">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_car_1f_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="70dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_car_1f_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="165dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_car_1f_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="270dp" />

    <ImageView
        android:id="@+id/image_hanlim3f"
        android:layout_width="wrap_content"
        android:layout_height="423dp"
        android:layout_marginTop="80dp"
        android:contentDescription="@string/todo"
        android:scaleType="fitXY"
        android:src="@drawable/hanlim3fmap"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="36dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text_hanlim3f2"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginTop="65dp"
        android:gravity="center"
        android:text="①체육관"
        android:textSize="22sp"
        app:layout_constraintTop_toBottomOf="@id/image_hanlim3f"
        app:layout_constraintStart_toStartOf="@id/guideline_start_car_1f_1"
        tools:ignore="MissingConstraints"
        android:clickable="true"
        android:focusable="true" />

    <TextView
        android:id="@+id/text_hongji3f3"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginTop="65dp"
        android:gravity="center"
        android:text="②샤워실1"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@id/guideline_start_car_1f_2"
        app:layout_constraintTop_toBottomOf="@id/image_hanlim3f"
        tools:ignore="MissingConstraints"
        />

    <TextView
        android:id="@+id/text_hanlim3f4"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginTop="65dp"
        android:gravity="center"
        android:text="③샤워실2"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@id/guideline_start_car_1f_3"
        app:layout_constraintTop_toBottomOf="@id/image_hanlim3f"
        tools:ignore="MissingConstraints"
        />

</androidx.constraintlayout.widget.ConstraintLayout>